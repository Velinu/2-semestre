algoritmo "7.Numeros de 10 a 50"

var
   n1,n2,n3: inteiro
inicio

      para n1 de 1 ate 15 faca
       escreva("digite um numero")
       leia(n3)
        se (n2<10) ou (n2>50) entao
         escreva("numero invalido")
        fimse
       n2<-n2+n3
      fimpara
      
      escreval(n2/15)

fimalgoritmo
