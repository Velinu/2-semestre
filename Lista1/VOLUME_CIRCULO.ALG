Algoritmo "Volume_Circulo"

Var
   Raio: real
   Altura: real
   Volume: real

Inicio
      escreval("Digite seu raio:")
      leia (Raio)
      escreval("Digite a altura do seu circulo:")
      leia (altura)
      Volume <- ((Raio*Raio)*3.1416)*Altura
      escreval("O volume de seu circulo é", Volume)


Fimalgoritmo