Algoritmo "Area_Triangulo"

Var
    altura: real
    largura: real
    area: real
    

Inicio
   escreval("Qual a altura de seu triangulo:")
   leia (altura)
   escreval("Qual a largura de seu triangulo:")
   leia (largura)
   area <- (altura*largura)/2
   escreva("A area de seu triangulo é:", area)
Fimalgoritmo