Algoritmo "16.Vogal"

Var

 pal,pal2:caracter
 n1,n2:inteiro

Inicio

  escreva("digite seu nome bro: ")
  leia(pal)

  para n1 de 1 ate compr(pal) faca
  pal2:=copia(pal,n1,1)

  se(pal2=maiusc(pal2)) entao
  se(pal2="A") ou (pal2="E") ou (pal2="I") ou (pal2="O") ou (pal2="U") entao
     n2<-n2+1
  fimse
  fimse
  fimpara

  escreva(n2)

Fimalgoritmo
